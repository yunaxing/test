<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android.support.v7.cardview="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#6e000000"
    tools:context="com.qkxmall.mall.views.address.AlertFragment">

    <!-- TODO: Update blank fragment layout -->
    <android.support.v7.widget.CardView
        android:id="@+id/card"
        android:layout_width="256dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android.support.v7.cardview:cardBackgroundColor="#FFFFFF"
        android.support.v7.cardview:cardCornerRadius="8dp"
        android.support.v7.cardview:cardElevation="8dp"
        >
        <ListView
            android:id="@+id/list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            ></ListView>
    </android.support.v7.widget.CardView>

</RelativeLayout>
